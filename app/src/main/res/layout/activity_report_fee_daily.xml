<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/grayF5F5F5"
    android:orientation="vertical">

    <include layout="@layout/layout_title_bar" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dp_15"
        android:background="@drawable/bg_radius_15_white"
        android:orientation="vertical"
        android:paddingTop="@dimen/dp_19"
        android:paddingBottom="@dimen/dp_19">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingStart="@dimen/dp_15"
            android:paddingEnd="@dimen/dp_15">

            <TextView
                android:id="@+id/tvParkingName"
                style="@style/textBlackStyle14"
                android:textColor="@color/blue629BFF"
                android:layout_width="wrap_content"
                android:layout_weight="1"
                android:layout_gravity="center_vertical"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tvDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_10"
                android:layout_gravity="center_vertical"
                android:gravity="end"
                android:textColor="@color/colorGraySmallText" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_0_5"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_19"
            android:layout_marginEnd="@dimen/dp_15"
            android:layout_marginBottom="@dimen/dp_19"
            android:background="@color/grayEEEEEE" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingStart="@dimen/dp_15"
            android:paddingEnd="@dimen/dp_15">

            <TextView
                style="@style/textBlackStyle14"
                android:text="收费员姓名"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tvTollManName"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_10"
                android:layout_weight="1"
                android:gravity="end"
                android:textSize="@dimen/sp_14"
                android:textColor="@color/colorGraySmallText" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_19"
            android:orientation="horizontal"
            android:paddingStart="@dimen/dp_15"
            android:paddingEnd="@dimen/dp_15">

            <TextView
                style="@style/textBlackStyle14"
                android:text="收费员编号"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tvTollManNum"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_10"
                android:layout_weight="1"
                android:gravity="end"
                android:textSize="@dimen/sp_14"
                android:textColor="@color/colorGraySmallText" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_0_5"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_18"
            android:layout_marginEnd="@dimen/dp_15"
            android:layout_marginBottom="@dimen/dp_19"
            android:background="@color/grayEEEEEE" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingStart="@dimen/dp_15"
            android:paddingEnd="@dimen/dp_15">

            <TextView
                style="@style/textBlackStyle14"
                android:text="总车次（次）"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tvTotalCarCount"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_10"
                android:layout_weight="1"
                android:gravity="end"
                android:textSize="@dimen/sp_14"
                android:textColor="@color/colorGraySmallText" />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_19"
            android:orientation="horizontal"
            android:paddingStart="@dimen/dp_15"
            android:paddingEnd="@dimen/dp_15">

            <TextView
                style="@style/textBlackStyle14"
                android:text="总应收（元）"
                android:textStyle="bold" />

            <!--<TextView
                style="@style/textBlackStyle14"
                android:text="［忽略］"
                android:textColor="@color/colorGraySmallText"
                android:textSize="@dimen/sp_12"
                android:textStyle="bold" />-->

            <TextView
                android:id="@+id/tvTotalShouldIncome"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_10"
                android:layout_weight="1"
                android:gravity="end"
                android:textSize="@dimen/sp_14"
                android:textColor="@color/colorGraySmallText" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_19"
            android:orientation="horizontal"
            android:paddingStart="@dimen/dp_15"
            android:paddingEnd="@dimen/dp_15">

            <TextView
                style="@style/textBlackStyle14"
                android:text="总实收（元）"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tvTotalReallyIncome"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_10"
                android:layout_weight="1"
                android:gravity="end"
                android:textSize="@dimen/sp_14"
                android:textColor="@color/colorGraySmallText" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_19"
            android:orientation="horizontal"
            android:paddingStart="@dimen/dp_15"
            android:paddingEnd="@dimen/dp_15">

            <TextView
                style="@style/textBlackStyle14"
                android:text="线上支付（元）"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tvOnlineIncome"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_10"
                android:layout_weight="1"
                android:gravity="end"
                android:textSize="@dimen/sp_14"
                android:textColor="@color/colorGraySmallText"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_19"
            android:orientation="horizontal"
            android:paddingStart="@dimen/dp_15"
            android:paddingEnd="@dimen/dp_15">

            <TextView
                style="@style/textBlackStyle14"
                android:text="线下支付（元）"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tvOffLineIncome"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_10"
                android:layout_weight="1"
                android:gravity="end"
                android:textSize="@dimen/sp_14"
                android:textColor="@color/colorGraySmallText"
                />
        </LinearLayout>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/llBottomLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="@dimen/dp_28"
            android:layout_marginEnd="@dimen/dp_28"
            android:layout_marginBottom="@dimen/dp_30"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/llFeeRecord"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/bg_select_primary_green"
                android:clickable="true"
                android:focusable="true"
                android:orientation="vertical"
                android:paddingTop="@dimen/dp_11"
                android:paddingBottom="@dimen/dp_11">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:orientation="horizontal">

                    <!-- <ImageView
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:layout_gravity="center_vertical"
                         android:src="@mipmap/ic_wallet_white" />-->

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_vertical"
                        android:layout_marginStart="@dimen/dp_5"
                        android:gravity="center"
                        android:letterSpacing="0.18"
                        android:text="收费记录"
                        android:textColor="@color/white"
                        android:textSize="@dimen/sp_14"
                        android:textStyle="bold" />
                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:id="@+id/llPayPrintCode"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_15"
                android:layout_weight="1"
                android:background="@drawable/bg_select_primary_blue"
                android:clickable="true"
                android:focusable="true"
                android:orientation="vertical"
                android:paddingTop="@dimen/dp_11"
                android:paddingBottom="@dimen/dp_11">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:orientation="horizontal">

                    <!--  <ImageView
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_gravity="center_vertical"
                          android:src="@mipmap/ic_pay_white_small" />-->

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_vertical"
                        android:layout_marginStart="@dimen/dp_5"
                        android:gravity="center"
                        android:letterSpacing="0.18"
                        android:text="打印凭条"
                        android:textColor="@color/white"
                        android:textSize="@dimen/sp_14"
                        android:textStyle="bold" />
                </LinearLayout>


            </LinearLayout>

        </LinearLayout>
    </RelativeLayout>
</LinearLayout>