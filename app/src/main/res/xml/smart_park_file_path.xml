<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--对应文件夹名需与file_string一一对应app开发者可以通过自定义file_string及file_path达到符合自己的规则-->
    <paths>
        <!--context.getExternalCacheDir()即:/storage/emulated/0/Android/data/<package-name>/cache/xxx/-->
        <external-cache-path
            name="smart_park_external_cache_dir"
            path="smart_park_external_cache_dir" />
        <!--context.getCacheDir()即:/data/data/<package-name>/cache/xxx/-->
        <cache-path
            name="smart_park_cache_dir"
            path="smart_park_cache_dir" />
        <!--Environment.getExternalStorageDirectory()即:/storage/emulated/0/xxx/-->
        <external-path
            name="external_storage_directory"
            path="external_storage_directory" />
        <!--context.getExternalFilesDir()即:/data/data/<package-name>/files/xxx/-->
        <external-files-path
            name="external_files_dir"
            path="external_files_dir" />
        <!--context.getFilesDir()即:/data/data/<package-name>/files/xxx/-->
        <files-path
            name="smart_park_files_dir"
            path="smart_park_files_dir" />
    </paths>
</resources>